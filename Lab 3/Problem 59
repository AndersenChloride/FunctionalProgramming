module Main (main) where
main::IO()
main =  do print $ show (cbalTree 1 1)
           print $ show (cbalTree 1 2)
           print $ show (cbalTree 1 3)
        
         
data Tree a = Empty | Branch a (Tree a) (Tree a) deriving (Show, Eq)
cbalTree :: a -> Int -> [Tree a]
cbalTree s 0 = [Empty]
cbalTree s 1 = [Branch s Empty Empty]
cbalTree s x = [Branch s c d | (a, b) <- [(x-2, x-1), (x-1, x-1), (x-1, x-2)], c <- cbalTree s a, d <- cbalTree s b]
